
{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/.*",
        "destination": "/api/index.html"
      },
      {
        "source": "/eventos/.*",
        "destination": "/eventos/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  